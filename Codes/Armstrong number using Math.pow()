
public class Practice{
    public static void main(String[] args){
        int num = 8208;
        String num1 = Integer.toString(num);
        int size = num1.length();
        int sum = 0;
        while(num!=0){
            int d = num%10;
            sum = sum + (int)Math.pow(d, size);
            num = num/10;
        }
        System.out.println(sum);
        
    }
}

