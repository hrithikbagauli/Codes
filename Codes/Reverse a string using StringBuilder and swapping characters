
public class Practice{
    public static void main(String[] args){
        StringBuilder sb = new StringBuilder("Hello12345");
        int j = sb.length()-1;
        char temp;
        for(int i=0; i<=j; i++){
            temp = sb.charAt(i);
            sb.setCharAt(i, sb.charAt(j));
            sb.setCharAt(j, temp);
            j--;
        }
        System.out.println(sb);
    }
}

